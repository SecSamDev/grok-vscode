{
    "name": "grok",
    "displayName": "Grok Pattern",
    "description": "grok pattern",
    "version": "0.0.1",
    "author": {
        "name": "Samuel Garces"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "grok",
                "aliases": [
                    "Grok",
                    "Grok pattern"
                ],
                "extensions": [
                    ".grok"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "grok",
                "scopeName": "source.grok",
                "path": "./syntaxes/grok.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "grok",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:grok"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.2.0"
    },
    "publisher": "secsamdev",
    "dependencies": {
        "grok-js": "^3.2.3"
    }
}
